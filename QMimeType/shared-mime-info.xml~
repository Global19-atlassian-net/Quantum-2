<mime-info xmlns='http://zester.github.com/Chipara/shared-mime-info'>

	<!-- Html File -->
	<mime-type value="text/html">
		<summery>Html Document</summery>
    	<description>HyperText Markup Language Document</description>
		<icon value="text-html"/>
    	<pattern value="html, htm"/>
		<application value=""/>
	</mime-type>

	<!-- Text File -->
	<mime-type value="text/plain">
		<summery>Text Document</summery>
    	<description>Plain Text Document</description>
		<icon value="text-x-generic"/>
    	<pattern value="txt"/>
		<application value=""/>
	</mime-type>

	<!-- PDF File -->
	<mime-type value="application/pdf">
		<summery>PDF Document</summery>
    	<description>Portable Document Format</description>
		<icon value="application-pdf"/>
    	<pattern value="pdf"/>
		<application value=""/>
	</mime-type>

	<!-- Shell Script -->
	<mime-type value="application/x-shellscript">
		<summery>Shell Script</summery>
    	<description></description>
		<icon value="text-x-script"/>
    	<pattern value="sh"/>
		<application value=""/>
	</mime-type>


</mime-info>
